variable "usersage" {
  type = map(any)
  default = {
    Radhe = 20
    Bhim  = 30
  }
}
variable "usersname" {
  type = string
}

output "printrage" {
  value = "My name is ${var.usersname} and My age is ${lookup(var.usersage, "${var.usersname}")}"
}

